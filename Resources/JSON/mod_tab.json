{
    "type": "QWidget",
    "name": "Mod",
    "layout": {
        "type": "QHBoxLayout",
        "children": [
            {
                "type": "QLabel",
                "bind": {
                    "text": "Edelweiss.Model.Name"
                }
            },
            {
                "type": "QPushButton",
                "text": "Change Model Name",
                "click": {
                    "method": "Edelweiss:MainInterop.ChangeModelName",
                    "args": ["new model name"]
                }
            },
            {
                "type": "QPushButton",
                "text": "Change Model Object",
                "click": {
                    "method": "Edelweiss:MainInterop.ChangeModelObject",
                    "args": []
                }
            },
            {
                "type": "QComboBox",
                "modifiable": true,
                "canEditDefaults": false,
                "bind": {
                    "options": "Edelweiss.Model.SampleList",
                    "defaults": "Edelweiss.Model.SampleList"
                }
            }
        ]
    }
}