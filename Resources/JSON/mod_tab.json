{
    "type": "QWidget",
    "name": "Mod",
    "layout": {
        "type": "QHBoxLayout",
        "children": [
            {
                "type": "QPushButton",
                "text": "Change Model Name",
                "click": {
                    "method": "Edelweiss:MainInterop.ChangeModelName",
                    "args": ["new model name"]
                }
            },
            {
                "type": "QPushButton",
                "text": "Change Model Object",
                "click": {
                    "method": "Edelweiss:MainInterop.ChangeModelObject"
                }
            },
            {
                "type": "QPushButton",
                "text": "Log Model",
                "click": {
                    "method": "Edelweiss:MainInterop.LogModelObject"
                }
            },
            {
                "type": "QPushButton",
                "text": "Change Submodel",
                "click": {
                    "method": "Edelweiss:MainInterop.ChangeSubmodel"
                }
            },
            {
                "type": "Form",
                "model": "Edelweiss.Model",
                "fields": [
                    "Name", "Directory", "Selectable", "Plugin", "Bool", "Int", "Submodel"
                ],
                "fieldInfo": {
                    "Selectable": {
                        "type": "list",
                        "options": ["Option 1", "Option 2", "Option 3"]
                    },
                    "Plugin": {
                        "type": "list",
                        "bind": {
                            "options": "Edelweiss.PythonPlugins"
                        }
                    },
                    "Submodel": {
                        "fields": ["SubName", "SubDirectory"],
                        "fieldLayout": [
                            ["SubName"],
                            ["SubDirectory"]
                        ]
                    }
                },
                "fieldLayout": [
                    ["Name", "Submodel"],
                    ["Directory", "Bool", "Int"],
                    ["Selectable", "Plugin"]
                ]
            }
        ]
    }
}