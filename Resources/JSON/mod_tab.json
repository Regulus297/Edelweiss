{
    "type": "QWidget",
    "name": "Mod",
    "layout": {
        "type": "QHBoxLayout",
        "children": [
            {
                "type": "QPushButton",
                "text": "madelin celets",
                "click": {
                    "method": "Edelweiss:MainInterop.SetCelesteDirectory",
                    "args": ["another celets directory"]
                }
            },
            {
                "type": "QLabel",
                "bind": {
                    "text": "Edelweiss:CelesteDirectoryPref.CelesteDirectory"
                }
            },
            {
                "type": "QLineEdit",
                "bind": {
                    "text": "Edelweiss:CelesteDirectoryPref.CelesteDirectory"
                }
            },
            {
                "type": "QLineEdit",
                "bind": {
                    "text": {
                        "property": "Edelweiss:CelesteDirectoryPref.CelesteDirectory",
                        "sync": false
                    }
                },
                "edit": {
                    "method": "Edelweiss:MainInterop.Debug",
                    "args": ["@text"]
                }
            },
            {
                "type": "QComboBox",
                "bind": {
                    "options": "Edelweiss.PythonPlugins"
                },
                "change": {
                    "method": "Edelweiss:MainInterop.Debug",
                    "args": ["@index"]
                }
            },
            {
                "type": "QComboBox",
                "modifiable": true,
                "canEditDefaults": false,
                "bind": {
                    "options": "Edelweiss.SampleList",
                    "defaults": "Edelweiss.SampleList"
                },
                "change": {
                    "method": "Edelweiss:MainInterop.Debug",
                    "args": ["@0"]
                }
            }
        ]
    }
}